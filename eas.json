{
	"cli": {
		"version": ">= 12.4.1",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"android": {
				"image": "latest"
			},
			"ios": {
				"image": "latest"
			},
			"env": {
				"EXPO_PUBLIC_BACKEND_BASEURL": "https://app-api.hakunamatata-me.com/api/User/"
			}
		},
		"development-simulator": {
			"extends": "development",
			"ios": {
				"simulator": true
			}
		},
		"preview": {
			"distribution": "internal",
			"channel": "main",
			"android": {
				"image": "latest"
			},
			"ios": {
				"image": "macos-sonoma-14.6-xcode-16.0"
			},
			"env": {
				"EXPO_PUBLIC_BACKEND_BASEURL": "https://app-api.hakunamatata-me.com/api/User/"
			}
		},
		"production": {
			"channel": "production",
			"autoIncrement": true,
			"android": {
				"image": "latest"
			},
			"ios": {
				"image": "macos-sonoma-14.6-xcode-16.0"
			},
			"env": {
				"EXPO_PUBLIC_BACKEND_BASEURL": "https://app-api.hakunamatata-me.com/api/User/"
			}
		}
	},
	"submit": {
		"production": {}
	}
}

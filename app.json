{
  "expo": {
    "name": "Hakuna Matata Supermarket App",
    "slug": "hakuna-matata",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cosmicclank.hakunamatata",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cosmicclank.hakunamatata"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "480e0064-9b40-40bd-a705-40c36c55c086"
      }
    },
    "owner": "cosmic_clank",
    "updates": {
      "url": "https://u.expo.dev/7e4a27ed-bec3-412a-ae9c-d1f37102ea9c"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
